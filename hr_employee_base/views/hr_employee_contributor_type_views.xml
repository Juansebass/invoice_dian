<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
      <record id="hr_employee_contributor_type_form" model="ir.ui.view">
      <field name="name">view.hr.employee.contributor.type.form</field>
      <field name="model">hr.employee.contributor.type</field>
      <field name="arch" type="xml">
        <form string="tipo">
           <group>
             <group>
                <field name="name" string="Tipo de Cotizante"/>
             </group>
           </group>   
        </form>
      </field>
    </record> 

    <record id="hr_employee_contributor_type_tree" model="ir.ui.view">
      <field name="name">view.hr.employee.contributor.type.tree</field>
      <field name="model">hr.employee.contributor.type</field>
        <field name="arch" type="xml">
            <tree string="sector tipo">
                <field name="name" string="Tipo de Cotizante"/>
            </tree>
        </field>
    </record>

    <record id="action_hr_employee_contributor_type" model="ir.actions.act_window">
        <field name="name">Tipo de Cotizante</field>
        <field name="res_model">hr.employee.contributor.type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            Clave Receptor
          </p>
        </field>
    </record>

    <menuitem name='Tipo de Cotizante'
            action='action_hr_employee_contributor_type'
            id='menu_hr_employee_contributor_type'
            parent='hr_recruitment.menu_hr_recruitment_configuration'
            sequence='25'/> 
    </data>   
</odoo>